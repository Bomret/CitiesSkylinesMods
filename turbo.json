{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "restore",
        "^build"
      ],
      "inputs": [
        "**/*.cs"
      ],
      "outputs": [
        "./bin/**",
        "./obj/**"
      ]
    },
    "publish": {
      "dependsOn": [
        "restore"
      ]
    },
    "test": {
      "dependsOn": [
        "restore"
      ]
    },
    "release": {
      "dependsOn": [
        "restore"
      ]
    },
    "restore": {}
  }
}