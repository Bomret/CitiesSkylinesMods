{
  "dependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@google/semantic-release-replace-plugin": "1.2.7",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "husky": "latest",
    "rimraf": "latest",
    "semantic-release": "22.0.12",
    "semantic-release-monorepo": "7.0.5",
    "turbo": "latest"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bomret/CitiesSkylinesMods.git"
  },
  "scripts": {
    "build": "dotnet build",
    "docs:serve": "dotnet tool run docfx docs/docfx.json --serve",
    "prepare": "husky install",
    "publish": "npx turbo publish --concurrency=1",
    "release": "npx turbo release --concurrency=1",
    "restore": "dotnet restore"
  },
  "version": "0.0.0",
  "workspaces": [
    "./mods/*",
    "./libs/*"
  ]
}